    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.2.4",
    "del": "^3.0.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.6.0",
    "monaco-editor": "^0.21.0",
    "path": "^0.12.7",
    "promise-polyfill": "^8.1.0",
    "raw-loader": "^4.0.2",
    "rimraf": "~3.0.2",
    "sass": "^1.49.9",
    "sass-loader": "^10.1.1",
    "style-loader": "^1.2.1",
    "tfx-cli": "^0.16.0",
    "ts-loader": "^4.1.0",
    "tslint": "^5.18.0",
