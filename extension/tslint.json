{
    "defaultSeverity": "error",
    
    "linterOptions": {
        "exclude": [
            "bin",
            "configs/**/*.js",
            "node_modules/**"
        ]
},
    "jsRules": {},
    "rules": {
        "max-line-length": [true, 200],
        "no-angle-bracket-type-assertion": false,
        "object-literal-sort-keys": false,
        "max-classes-per-file": false,
        "no-bitwise": false,
        "ban-types": false,
        "member-ordering": false,
        "forin": false,
        "no-namespace": false,
        "no-unused-expression": true,
        "no-console": false,
        "semicolon": false,
        "whitespace": false,
        "quotemark": false
    },
    "rulesDirectory": []
}